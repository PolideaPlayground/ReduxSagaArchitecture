{
    "parser": "babel-eslint",
    "plugins": ["prettier", "flowtype", "react", "react-native"],
    "parserOptions": {
      "ecmaVersion": 7,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "extends" : [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:flowtype/recommended"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
}